<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.gueei.demo.contactmanager"
      android:versionCode="2"
      android:versionName="1.1">
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="@string/app_name" android:name="ContactManagerApplication">
    	<activity android:name="ContactManager">
    		<intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ContactAdder" android:label="@string/addContactTitle">
        </activity>
	</application>
    <uses-sdk android:minSdkVersion="7" />
	<uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
</manifest> 