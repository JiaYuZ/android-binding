<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.gueei.demo.musicplayer"
      android:versionName="1.01" android:versionCode="2">
    <application 
     android:icon="@drawable/icon" android:label="@string/app_name" android:name="MusicPlayerApplication" android:debuggable="true">
        <activity android:label="@string/app_name" android:name=".MusicListActivity" android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <service android:name="MusicPlayerService"></service>
	<activity android:name="Explain"
		android:theme="@android:style/Theme.Dialog" android:launchMode="singleTask"></activity>
</application>
    <uses-sdk android:minSdkVersion="7" />

</manifest> 