<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.gueei.demo.musicplayer"
      android:versionCode="1"
      android:versionName="1.0">
    <application 
     android:icon="@drawable/icon" android:label="@string/app_name" android:name="MusicPlayerApplication">
        <activity android:name=".MusicList"
                  android:label="@string/app_name" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <service android:name="MusicPlayer"></service>
	<activity android:name="Explain"
		android:theme="@android:style/Theme.Dialog"
	></activity>
</application>
    <uses-sdk android:minSdkVersion="7" />

</manifest> 